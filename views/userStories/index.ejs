<h3> Bienvenue dans vos User Stories</h3>

<ul class="mdl-list"></ul>
<% userStories.forEach(userStory=> {%>
    <li class="mdl-list__item">
        <span class="mdl-list__item-primary-content">
            <i class="material-icons mdl-list__item-icon">dashboard</i>
            <%= userStory.title %>
        </span>
        <a href="/userStories/update/<%= userStory._id %>" class="mdl-button mdl-js-button mdl-button--fab mdl-button">
            <i class="material-icons">edit</i>
        </a>
        <a href="/userStories/delete/<%= userStory._id %>" class="mdl-button mdl-js-button mdl-button--fab mdl-button--accent">
            <i class="material-icons">delete</i>
        </a>
    </li>
    <%}); %>
</ul>