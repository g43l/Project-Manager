<h3> Bienvenue dans vos Projets</h3>

<ul class="mdl-list"></ul>
<% projects.forEach(project=> {%>
    <li class="mdl-list__item">
        <span class="mdl-list__item-primary-content">
            <i class="material-icons mdl-list__item-icon">dashboard</i>
            <%= project.name %>
        </span>
        <a href="/projects/update/<%= project._id %>" class="mdl-button mdl-js-button mdl-button--fab mdl-button">
            <i class="material-icons">edit</i>
        </a>
        <a href="/projects/delete/<%= project._id %>" class="mdl-button mdl-js-button mdl-button--fab mdl-button--accent">
            <i class="material-icons">delete</i>
        </a>
    </li>
    <%}); %>
</ul>